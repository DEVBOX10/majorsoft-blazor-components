<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
	<TargetFramework>net5.0</TargetFramework>
	<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	<PackageId>Majorsoft.Blazor.Server.Logging.Console</PackageId>
	<Version>1.0.0-preview.8.20417.9</Version>
	<Authors>Imre Toth</Authors>
	<Product>Majorsoft Blazor Components</Product>
	<Description>Blazor extension for logging to browser console.  WARNING: only works for apps using Server Hosting Model!</Description>
	<Copyright>Imre Toth 2020</Copyright>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<PackageProjectUrl>https://github.com/majorimi/blazor-components</PackageProjectUrl>
	<RepositoryUrl>https://github.com/majorimi/blazor-components</RepositoryUrl>
	<PackageIcon>blazor.components.png</PackageIcon>
	<RepositoryType>Git</RepositoryType>
	<AssemblyVersion>0.9.0.0</AssemblyVersion>
	<FileVersion>0.9.0.0</FileVersion>
	<PackageTags>.Net5 Blazor Server ConsoleLog</PackageTags>
	<PackageReleaseNotes>First release using .NET 5 preview.8</PackageReleaseNotes>
	<Title>Blazor Server Logging Console</Title>
  </PropertyGroup>


  <ItemGroup>
	<PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="5.0.0-preview.8.20414.8" />
	<PackageReference Include="Microsoft.Extensions.Logging" Version="5.0.0-preview.8.20407.11" />
  </ItemGroup>


  <ItemGroup>
	<None Include="..\..\.github\Images\blazor.components.png">
	  <Pack>True</Pack>
	  <PackagePath></PackagePath>
	</None>
  </ItemGroup>

</Project>
